class Dog extends Pet {
    private String name, race;
    private int age;

    Dog(String name, String race, int age) {
        this.name = name;
        this.race = race;
        this.age = age;
    }
    
    @Override
    public void makeNoise() {
        System.out.println(name + " HAS BARKED!!!!" );
    }

}
